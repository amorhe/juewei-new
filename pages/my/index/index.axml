<view class="myContent">
  <view class="myTop flexNormal">
    <view class="flexSpecial">
      <image class="avatar" mode="scaleToFill" src="{{ userInfo.head_img||avatarImg }}"/>
      <view class="fontFourtyWeight" onTap="isloginFn">
        <!-- <text a:if="{{ userInfo.nick_name }}">{{ userInfo.nick_name }}</text>
        <text a:else>绝味会员</text> -->

        <text>{{ loginId==30106?'登录':userInfo.nick_name?userInfo.nick_name:'绝味会员' }}</text>
      </view>
      <!-- <view a:else class="fontFourtyWeight" onTap="toUrl" data-url='/pages/login/auth/auth'>登录</view> -->
    </view>
    <image class="ImageFourty" mode="scaleToFill" src="{{imageUrl}}settings.png" onTap="toUrl" data-url="/package_my/pages/mycenter/mycenter"/>
  </view>

  <view class="vipView">
    <view class="flexNormal">
      <view>会员卡</view>
      <image class="normalImage" mode="scaleToFill" src="{{imageUrl}}qrcode.png" onTap="toUrl" data-url="/package_my/pages/membercard/membercard"/>
    </view>
     <image class="vipcardPng" mode="scaleToFill" src="{{imageUrl}}position_tit.png"/>
     <image class="qingxie" mode="scaleToFill" src="../../common/img/qingxie.png"/>
  </view>
  <!-- 下面的菜单 -->
  <view class="my_menu">
    <view class="fontBold">我的服务</view>
    <view class="menuList">
      <view onTap="toUrl" data-url='/package_my/pages/coupon/coupon'>
        <view>
         <view>
            <image mode="scaleToFill" src="{{imageUrl}}my_coupon.png"/>
            <view>卡券</view>
         </view>
        </view>
      </view>
      <view onTap="toUrl" data-url='/package_vip/pages/pointlist/pointlist'>
        <view>
          <view>
            <image mode="scaleToFill" src="{{imageUrl}}my_points.png"/>
            <view>积分</view>
          </view>
        </view>
      </view>
      <view onTap="toUrl" data-url='/package_my/pages/myaddress/myaddress'>
        <view>
          <view>
            <image mode="scaleToFill" src="{{imageUrl}}my_address.png"/>
            <view>地址</view>
          </view>
        </view>
      </view>
      <view onTap="toUrl" data-url='/package_my/pages/nearshop/nearshop'>
        <view>
          <view>
            <image mode="scaleToFill" src="{{imageUrl}}my_nearshop.png"/>
            <view>附近门店</view>
          </view>
        </view>
      </view>
      <view onTap="toUrl" data-url='/package_my/pages/entitlement/entitlement'>
        <view>
          <view>
            <image mode="scaleToFill" src="{{imageUrl}}menu_vip2.png"/>
            <view>专享权益</view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>

<!-- <navigator url="/package_my/pages/onlineservice/onlineservice">
  <image class="customerService" mode="scaleToFill" src="{{imageUrl}}service.png"/>
</navigator> -->
<image class="customerService" mode="scaleToFill" onTap="makePhoneCall" src="{{imageUrl}}service.png"/>